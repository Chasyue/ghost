<h1>User</h1>

<table class="record_properties">
    <tbody>
    <tr>
        <th>Username</th>
        <td>{{ user.username }}</td>
    </tr>
    <tr>
        <th>Password</th>
        <td>{{ user.password }}</td>
    </tr>
    <tr>
        <th>Salt</th>
        <td>{{ user.salt }}</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>{{ user.name }}</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>{{ user.email }}</td>
    </tr>
    <tr>
        <th>Created</th>
        <td>{{ user.created }}</td>
    </tr>
    <tr>
        <th>Id</th>
        <td>{{ user.id }}</td>
    </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('user') }}"> Back to the list </a>
    </li>
    <li>
        <a href="{{ path('user_edit', { 'id': user.id }) }}"> Edit </a>
    </li>
</ul>
